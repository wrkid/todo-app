{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends" : [
      "airbnb",
      "plugin:react/recommended",
      "prettier"
    ],
    "parserOptions": { 
      "ecmaFeatures": { 
        "jsx": true 
      }, 
      "ecmaVersion": "latest", 
      "sourceType": "module" 
    },
    "plugins": [
      "react",
      "prettier"
    ],
    "rules": {
      "prettier/prettier": "error",
      "comma-dangle": 0
    }
}
